<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="items.News" table="NewsH">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="chatId" column="chatId" type="int"/>
        <property name="dateOfCreating" column="dateOfCreating" type="string"/>
        <property name="context" column="context" type="string"/>
    </class>
</hibernate-mapping>